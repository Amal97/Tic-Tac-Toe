
public class Game {
	private int board[][] = new int [3][3];
	
	private static void boardPrinter(int x, int y) {
		for(int i = 0; i < 3; i++) {
			for(int j = 0; j < 3; j++) {
				System.out.print(".");
			}
			System.out.println("");
		}
	}
	
	public static void main(String[] args) {
		boardPrinter(0,0);
	}
	
	
}
